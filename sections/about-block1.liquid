         },
         {
            "id":"why-acme-paragraph",
            "type":"text",
            "label":"paragraph",
           "default":"your-paragraph"
         }

		
],
	"presets": [
                {
                    "category": "about-block1",
                    "name":"support-section"

                }
            ]



  }
