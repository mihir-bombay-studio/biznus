            <div class="about-top-block">
                <div class="why-acme-block">
                    <div class="why-acme-small-text">{{section.settings.why-acme-small-text}}</div>
                    <h2 class="why-acme-heading">{{section.settings.why-acme-heading}}</h2>
                    <p class="why-acme-paragraph">{{section.settings.why-acme-paragraph}}</p>
